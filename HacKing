StartAllVariables(){
  ##################################
  #-----------) Colors (-----------#
  ##################################
  ColorsVariables(){
    nc="\033[00m"
    white="\033[1;37m"
    grey="\033[0;37m"
    purple="\033[0;35m"
    red="\033[31m"
    green="\033[32m"
    yellow="\033[33m"
    purple="\033[0;35m"
    cyan="\033[0;36m"
    cafe="\033[0;33m"
    fiuscha="\033[0;35m"
