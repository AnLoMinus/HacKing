  #    Bar Variables     #
  ########################
  BarVariables(){
    vBar="${RESETBG}${grey}[â–ˆ â–“ â–‘ â–’ â–’â–’â–’â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–’â–’â–’ â–’ â–‘ â–“ â–ˆ${orange}]${RESETBG}\n"
