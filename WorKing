######MainMenu######
main(){
  ManBanner
  echo "\t$cInfo Please select an action $cInfo\n"
  echo "\t ${c1} ${cafe} AnonSurf"
  echo "\t ${c2} ${cafe} Forensics"
  echo "\t ${c3} ${cafe} FramXploit"
  echo "\t ${c4} ${cafe} HasHack"
  echo "\t ${c5} ${cafe} InfoGath"
  echo "\t ${c6} ${cafe} PayLoads"
  echo "\t ${c7} ${cafe} PhisHack"
  echo "\t ${c8} ${cafe} PrivEsc"
  echo "\t ${c9} ${cafe} RemoteAccess"
  echo "\t ${c10} ${cafe} ReversEngine"
  echo "\t ${c11} ${cafe} Steganograhy"
  echo "\t ${c12} ${cafe} VulnHack"
  echo "\t ${c13} ${cafe} WebHack"
  echo "\t ${c14} ${cafe} WireLess"
  echo "\t ${c15} ${cafe} XInstKits"
  echo "\t ${cq} ${cafe} Quit to Main Menu "
  echo ""
  echo "\t$currentDir $(pwd)"
  echo "  $promptOption " && read -p ":" choice
	if [[ $choice == '1' ]]; then AnonSurf; fi
	if [[ $choice == '2' ]]; then Forensics; fi
	if [[ $choice == '3' ]]; then FramXploit; fi
	if [[ $choice == '4' ]]; then HasHack; fi
	if [[ $choice == '5' ]]; then InfoGath; fi
	if [[ $choice == '6' ]]; then PayLoads; fi
	if [[ $choice == '7' ]]; then PhisHack; fi
	if [[ $choice == '8' ]]; then PrivEsc; fi
	if [[ $choice == '9' ]]; then RemoteAccess; fi
	if [[ $choice == '10' ]]; then ReversEngine; fi
	if [[ $choice == '11' ]]; then Steganograhy; fi
  if [[ $choice == '12' ]]; then VulnHack; fi
  if [[ $choice == '13' ]]; then WebHack; fi
  if [[ $choice == '14' ]]; then WireLess; fi
  if [[ $choice == '15' ]]; then XInstKits; fi
	if [[ $choice == 'Q' || $choice == 'q' ]] ; then
		echo "$cInfo Quit to Main Menu"
    clear
    exit
	fi
    echo "$cInfo Returning to main menu..."
    sleep 0.3
    clear && main
}
######MainMenu######
#   Functions   #

#   Options   #
if [ "$1" == "start" ]; then
    start
elif [ "$1" == "install" ]; then
    install
elif [ "$1" == "remove" ]; then
    remove
else
  main
fi
