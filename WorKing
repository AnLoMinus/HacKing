uSage="$red[$cyan USAGE $red]$blue"
currentDir="$red[$grey Current Dir $red]$cyan "
installingDir="$red[$grey Installing Dir $red]$cyan"
cSharp="$red[$yellow#$red]$white"
cInfo="$red[$yellow!$red]$yellow"
cPlus="$red[$green+$red]$white"
cx="$red[$green$nx$red]$cafe"
cq="$red[$green$nq$red]$cafe"
ca="$red[$green$na$red]$cafe"
